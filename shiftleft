S_0x146f045d0 .scope module, "shiftleft_tb" "shiftleft_tb" 2 3;
 .timescale -9 -12;
P_0x146f04740 .param/l "DATA_WIDTH" 0 2 5, +C4<00000000000000000000000000010100>;
P_0x146f04780 .param/l "SHIFT_WIDTH" 0 2 6, +C4<00000000000000000000000000000101>;
v0x146f14d90_0 .var "data_in", 19 0;
v0x146f14e40_0 .net "data_out", 19 0, v0x146f14bd0_0;  1 drivers
v0x146f14ef0_0 .var "shift_amount", 4 0;
S_0x146f04910 .scope module, "uut" "shiftleft" 2 13, 3 1 0, S_0x146f045d0;
 .timescale 0 0;
    .port_info 0 /INPUT 20 "data_in";
    .port_info 1 /INPUT 5 "shift_amount";
    .port_info 2 /OUTPUT 20 "data_out";
v0x146f04b10_0 .net "data_in", 19 0, v0x146f14d90_0;  1 drivers
v0x146f14bd0_0 .var "data_out", 19 0;
v0x146f14c80_0 .net "shift_amount", 4 0, v0x146f14ef0_0;  1 drivers
E_0x146f04ad0 .event anyedge, v0x146f04b10_0, v0x146f14c80_0;
    .scope S_0x146f04910;
T_0 ;
    %wait E_0x146f04ad0;
    %load/vec4 v0x146f04b10_0;
    %ix/getv 4, v0x146f14c80_0;
    %shiftl 4;
    %store/vec4 v0x146f14bd0_0, 0, 20;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x146f045d0;
T_1 ;
    %pushi/vec4 699050, 0, 20;
    %store/vec4 v0x146f14d90_0, 0, 20;
    %pushi/vec4 3, 0, 5;
    %store/vec4 v0x146f14ef0_0, 0, 5;
    %delay 10000, 0;
    %vpi_call 2 25 "$display", "Test case 1 - Data Out: %b", v0x146f14e40_0 {0 0 0};
    %pushi/vec4 567466, 0, 20;
    %store/vec4 v0x146f14d90_0, 0, 20;
    %pushi/vec4 7, 0, 5;
    %store/vec4 v0x146f14ef0_0, 0, 5;
    %delay 10000, 0;
    %vpi_call 2 32 "$display", "Test case 2 - Data Out: %b", v0x146f14e40_0 {0 0 0};
    %end;
    .thread T_1;
